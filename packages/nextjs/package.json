{
  "name": "@safe-routes/nextjs",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "description": "Type-safe route utilities for Next.js",
  "keywords": [
    "nextjs",
    "typescript",
    "routing",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Uki884/safe-routes"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "vite build",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "vite build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "bin": {
    "safe-routes": "./dist/cli.js"
  },
  "files": [
    "dist",
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "types": "./index.d.ts",
      "import": "./index.mjs"
    }
  },
  "peerDependencies": {
    "next": ">=13.4.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "chokidar": "4.0.3",
    "commander": "13.1.0"
  },
  "devDependencies": {
    "@types/node": "22.13.4",
    "vite": "6.1.0",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  }
}