{
  "name": "next-typesafe-path",
  "license": "MIT",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "type": "module",
  "sideEffects": false,
  "description": "Type-safe path for Next.js",
  "keywords": [
    "nextjs",
    "typescript",
    "routing",
    "type-safe",
    "navigation",
    "search params",
    "query params"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uki884/next-typesafe-path.git",
    "directory": "packages/next-typesafe-path"
  },
  "scripts": {
    "prepare": "vite build",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:once": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "next-typesafe-path": "dist/cli.js"
  },
  "files": [
    "dist",
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "types": "./index.d.ts",
      "import": "./index.mjs"
    }
  },
  "peerDependencies": {
    "next": ">=13.4.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "chokidar": "4.0.3",
    "commander": "13.1.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/node": "22.13.4",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vite-plugin-dts": "4.5.3",
    "vitest": "3.0.5"
  }
}
