{
  "name": "@next-typesafe-path/example",
  "private": true,
  "scripts": {
    "dev": "npm run dev:next & npm run dev:path",
    "dev:next": "next dev",
    "dev:path": "next-typesafe-path --watch --trailing-slash false --config-dir ./utils",
    "build": "next-typesafe-path --trailing-slash false --config-dir ./utils && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next-typesafe-path": "workspace:*",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "typescript": "5.3.3"
  }
}
