{
  "name": "@safe-routes/example",
  "private": true,
  "scripts": {
    "dev": "npm run dev:safe-routes & npm run dev:next",
    "dev:next": "next dev",
    "dev:safe-routes": "safe-routes --watch --trailing-slash false",
    "build": "safe-routes && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@safe-routes/nextjs": "workspace:*",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "typescript": "5.3.3"
  }
}
