{
  "name": "@safe-routes/monorepo",
  "private": true,
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "lint": "biome lint --apply ./",
    "format": "biome format --apply ./",
    "fix": "biome check --apply ./",
