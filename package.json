    "test": "pnpm -r test:once",
    "build": "pnpm -r build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@changesets/cli": "2.27.1",
    "@changesets/changelog-github": "0.5.0"
  }
}